{
  "name": "cloud-atlas-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "generate-env": "node src/environments/generate-environment.js",
    "start:local": "ng serve --port 4200 --configuration local",
    "start:test": "ng serve --port 4200 --configuration test",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test-ui": "jest"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/e2e/",
      "<rootDir>/src/environments/"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.1.1",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/google-maps": "^19.1.1",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@ngrx/operators": "^19.0.0",
    "@ngrx/signals": "^19.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/google-maps": "^3.2.6",
    "angular-auth-oidc-client": "^19.0.0",
    "aws-rum-web": "^1.20.0",
    "postcss": "^8.5.1",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.4",
    "@angular/cli": "^19.1.4",
    "@angular/compiler-cli": "^19.1.0",
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.50.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jasmine-core": "~5.5.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.3",
    "typescript": "~5.7.2"
  }
}